{
  "name": "croatia-mobile-app",
  "version": "2023.7.0",
  "private": true,
  "scripts": {
    "postinstall": "npx patch-package && (test -n \"$IS_CI_ANDROID\" || (react-native setup-ios-permissions && npx pod-install))",
    "android": "react-native run-android --variant developDebug --appId com.croatiamobileapp.develop",
    "ios": "react-native run-ios --scheme CroatiaMobileApp --mode Debug",
    "xcode": "open ios/CroatiaMobileApp.xcworkspace",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:ci": "eslint --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release:test": "BUILD_ENVIRONMENT=test sh scripts/build-for-nexus.sh",
    "release:eit": "BUILD_ENVIRONMENT=eit sh scripts/build-for-nexus.sh"
  },
  "reactNativePermissionsIOS": [
    "AppTrackingTransparency",
    "Notifications"
  ],
